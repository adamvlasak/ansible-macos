---
user:
  login: foobar
  email: john@doe.org
  full_name: John Doe
  group: staff

font_smoothing: 0
editor: nvim
shell: /opt/homebrew/bin/zsh
path:
  - /opt/homebrew/bin
  - /opt/homebrew/sbin
  - /opt/homebrew/opt/curl/bin
  - /Users/{{ user.login }}/.dotnet/tools
  - ./node_modules/.bin
  - ${PATH}

aliases:
  - { alias: "u", command: "brew update && brew upgrade && brew autoremove && brew cleanup" }
  - { alias: "la", command: "ls -la" }
  - { alias: "ll", command: "ls -l" }
  - { alias: "vim", command: "nvim" }
  - { alias: "resetdock", command: "defaults write com.apple.dock ResetLaunchPad -bool true; killall Dock" }
  - { alias: "renoise", command: "/Applications/Renoise.app/Contents/MacOS/Renoise" }

zsh:
  plugins:
    - git
    - colored-man-pages
    - z
vim:
  colorscheme: desert
  plugins:
    - vim-syntastic/syntastic
    - pearofducks/ansible-vim
    - leafgarland/typescript-vim
    - pangloss/vim-javascript
    - vim-airline/vim-airline
    - junegunn/fzf
    - junegunn/fzf.vim

network:
  gateway: 192.168.10.1
  nameserver: 192.168.10.250
  subnet: 192.168.10.0/24
  interface: en0

pf:
  skipped_interfaces: ["lo0"]

homebrew:
  packages:
    - convmv
    - cuetools
    - curl
    - dos2unix
    - ffmpeg
    - flac
    - fzf
    - gawk
    - git
    - glances
    - go
    - grep
    - helm
    - hugo
    - jq
    - kind
    - lame
    - lua
    - make
    - midnight-commander
    - minikube
    - moreutils
    - neovim
    - nmap
    - node
    - pandoc
    - python
    - qemu
    - ripgrep
    - rsync
    - shntool
    - sox
    - sqlite
    - telnet
    - tig
    - tmux
    - tree
    - w3m
    - watch
    - wget
    - youtube-dl
    - zsh
    - zsh-completions

  cask_packages:
    - blackhole-16ch
    - calibre
    - discord
    - docker
    - dotnet-sdk
    - firefox
    - iina
    - iterm2
    - keepingyouawake
    - signal
    - suspicious-package
    - the-unarchiver
    - visual-studio-code
